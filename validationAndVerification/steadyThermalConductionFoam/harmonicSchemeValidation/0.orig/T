FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 293;
    }

    right
    {
        type            fixedValue;
        value           uniform 263;
    }

    side
    {
        type            empty;
        value           $internalField;
    }
}

