FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         none;
}

divSchemes
{
    default         none;
}

laplacianSchemes
{
    default         none;
   laplacian(k,T) Gauss harmonic uncorrected;
//   laplacian(k,T) Gauss linear uncorrected;  
}

interpolationSchemes
{
    default         none;
}

snGradSchemes
{
    default         none;
}


